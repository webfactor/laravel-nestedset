{
    "name": "webfactor/laravel-nestedset",
    "type": "library",
    "description": "Tree structured Elquent Models",
    "keywords": [
        "webfactor",
        "laravel",
        "nested sets",
        "tree",
        "database",
        "eloquent",
        "hierarchy",
        "parent",
        "children",
        "ancestor",
        "descendant",
        "sibling"
    ],
    "homepage": "https://github.com/webfactor/laravel-nestedset",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Ziegler",
            "email": "oliver.ziegler@webfactor.de",
            "homepage": "https://webfactor.de",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/events": "~5.5",
        "php": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Webfactor\\LaravelNestedSet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webfactor\\LaravelNestedSet\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webfactor\\Laravel\\NestedSet\\NestedSetServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
